<div class='card' style="background-color: <%= note.color.downcase %>;" data-note-id="<%= note.id %>" id="card-<%= note.id %>" >
  <div class='card-body' tabindex="-1" >
    <div class='row'>
      <div class='col'>
        <strong><%= note.title %></strong>
      </div>
    </div>
    <div class='row'>
      <div class='col'>
        <%= (note.content) %>
      </div>
    </div>
    <div class='row'>
      <div class='col'>
        <ul>
          <% note.labels.each do |label| %>
            <li><%= label.label_name %></li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>
